{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "off",
    "no-await-in-loop": "off",
    "camelcase": "off"
  }
}
